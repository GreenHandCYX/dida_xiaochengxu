/**index.wxss**/
@import '../order/order.wxss';

.myBtn{
  width: 129rpx;
  height: 96rpx;
  position: absolute;
  top: 360rpx;
  right: 0;
  margin-top: -40rpx;
}
.tooltip{
  padding:0 15rpx;
  height: 40rpx;
  position: absolute;
  left: 50%;
  top:50%;
  transform: translate(-50%,-250%);
  text-align: center;
  background-color: #333;
  border-radius: 20rpx;
  box-sizing: border-box;
  color:#fff;
  font-size: 13px;
   line-height:40rpx;
}

.tooltip .tooltipText{
  width: 100%;
  border: none;
  height: 100%;
  
   z-index: 99
}
.changeReturn{
  
  position: absolute;
  font-family: 'PingFangSC-Regular';
  bottom: 20vh;
  left: 0;
  width: 196rpx;
  height: 96rpx;
 
}
.returnhomeBg{
  width: 100%;
  height: 100%;
}



















