/* pages/order/order.wxss */

.orderStatus-bottom {
  width: 100vw;
  position: absolute;
  bottom: 0rpx;
  background: #fff;
}

.orderInfo {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  height: 184rpx;
}

.orderDetail {
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-content: center;
  margin-right: 140rpx;
}

.orderDetail text {
  font-family: PingFangSC-Regular;
}

.orderDetail .diverName {
  color: #222;
  font-size: 34rpx;
}

.orderDetail .diverCar {
  height: 34rpx;
  width: 134rpx;
  color: #fff;
  font-size: 26rpx;
  background: #848484;
  line-height: 34rpx;
  text-align: center;
}

.orderDetail .diverCompany {
  color: #999;
  font-size: 26rpx;
}

.diverImg {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin: 0 26rpx 0 30rpx;
}

.call {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}

.orderBtn {
  border: 1px solid rgba(5, 5, 5, 0.08);
}

.orderBtn::after {
  border-radius: 0;
  border: 1px solid rgba(5, 5, 5, 0.08);
}

.orderAddress-top {
  width: 100vw;
  background: #fff;
}

.route-start, .route-end {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  margin-left: 30rpx;
  padding-left: 20rpx;
}

.line {
  width: 95%;
  height: 1rpx;
  background: #e5e5e5;
  float: right;
}

.route-start {
  /* border-bottom: 1px solid rgb(229,229,229); */
  font-size: 0;
}

.route-start cover-view, .route-end cover-view {
  /* width: 100%; */
  display: inline-block;
  /* height: 88rpx; */
  line-height: normal;
  /* padding-right:20rpx; */
}

.route-start-icon, .route-end-icon {
  display: inline-block;
  margin-right: 40rpx;
  line-height: normal;
}

.vertical-align {
  font-size: 0;
}

.vertical-ref {
  display: inline-block;
  width: 0;
  vertical-align: middle;
}

.vertical-middle {
  display: inline-block;
  font-size: 34rpx;
  vertical-align: middle;
  line-height: normal;
}

.vertical-price {
  height: 88rpx;
}

.payWindow-extfee {
  color: #999;
  font-size: 26rpx;
}

.payWindow-input {
  height: auto !important;
  text-align: center;
  color: #ff7500;
  border-bottom: 1rpx solid #ececec;
}

.payWindow-top:first-child {
  line-height: 90rpx;
  font-size: 34rpx;
}

.payWindow-top:nth-child(2) {
  text-align: center;
  line-height: 90rpx;
}

.aaa {
  height: 88rpx;
  width: 0;
  vertical-align: middle;
}

.bbb {
  vertical-align: middle;
}
.gray{
  color: #ff7500;
}

.ccc {
  width: 80%;
  font-size: 34rpx;
  vertical-align: middle;
   white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.ccc .wx-cover-view {
  vertical-align: top;
}

.route-countdown, .route-price {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  border-top: 1px solid #e5e5e5;
  font-size: 34rpx;
  color: #777c8b;
  font-family: PingFangSC-Regular;
  padding-left: 30rpx;
}

.route-price {
  background: #fff;
  position: absolute;
  bottom: 114rpx;
  box-sizing: border-box;
  width: 100%;
  height: 88rpx;
  padding-right: 30rpx;
  text-align: center;
  line-height: 88rpx;
}

.route-countdown-num {
  display: inline-block;
  height: 110rpx;
  line-height: 110rpx;
}

.secNum {
  width: 120rpx;
  margin-right: 5rpx;
}

.secNumText {
  width: 540rpx;
}

.route-price-num, .route-price-tip, .route-price-coupon {
  line-height: normal;
  color: #333;
  font-size: 26rpx;
}

.route-price-num cover-view, .route-price-tip cover-view,
.route-price-coupon cover-view {
  font-size: 26rpx;
  font-family: PingFangSC-Light;
}

.route-price-num {
  margin: 0 20rpx;
}

.route-price-tip {
  color: #848484;

  /* 292d39 */
}
.route-price-tip cover-view:nth-child(1){
  color: #777c8b;
}
.route-price-tip cover-view:nth-child(2),
.route-price-coupon cover-view:nth-child(2) {
  color: rgb(255, 122, 52);
}
.route-price-num,.route-price-coupon{
  color: #292d39;
}
.route-price-num cover-view:nth-child(2) {
     color: rgb(66, 66, 66);
  font-weight: bold;
  /* width: 100rpx; */
}

.orderStatus-top {
  width: 100vw;
  background: #fff;
  height: 120rpx;
  color: #777c8b;
  font-size: 34rpx;
  padding-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.orderStatus-paytime {
  color: #bdc0cb;
  font-size: 26rpx;
  vertical-align: bottom;
  /* display: flex;
flex-direction: row; *//* justify-content:flex-start; */
}
.orderStatus-top-text{
margin-bottom: 8rpx;
}
.orderStatus-top-time{
margin-top: 8rpx;
}

.orderStatus-paytime-num {
  vertical-align: bottom;
  display: inline-block;
}

.orderStatus-bottom-payinfo-base {
  color: #666;
  font-size: 26rpx;
}

.orderStatus-bottom-payinfo-base-num {
  position: relative;
  top: 6rpx;
  font-size: 50rpx;
  margin: 0 10rpx;
  color: #666;
}

.payinfo-extfee {
  line-height: 60rpx;
  font-size: 26rpx;
  color: #666;
}
.payinfo-extfee cover-view:nth-child(1) {
width: 50rpx;
}

.orderBtn-bottom {
  position: absolute;
  bottom: 0;
  left: 0;
}

